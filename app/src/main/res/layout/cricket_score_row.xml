                </LinearLayout>

        </LinearLayout>
    <View
        android:id="@+id/invisibleLine1"
        android:visibility="invisible"
        android:layout_width="match_parent"
        android:layout_margin="@dimen/_5sdp"
        android:layout_height="@dimen/dimen_1dp"
        android:background="@color/light_view"/>

</LinearLayout>
</LinearLayout>