<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:id="@+id/cardView1"
                android:gravity="center"
                android:layout_height="match_parent"
                android:layout_marginHorizontal="@dimen/_10sdp"
                android:layout_marginVertical="@dimen/dimen_8dp"
                android:orientation="horizontal">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:layout_width="@dimen/dimen_18dp"
                    android:layout_height="@dimen/dimen_18dp"
                    android:layout_gravity="center"
                    android:src="@drawable/icon_four" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_14sp"
                    android:layout_weight="1"
                    android:ellipsize="end"
                    android:fontFamily="@font/euclid_circular_a_medium"
                    android:gravity="center|left"
                    android:maxLines="1"
                    android:text="@string/heading_line"
                    android:textColor="@color/home_font_color"
                    android:textSize="@dimen/_14sp" />

                <Button
                    android:id="@+id/arrowDown1"
                    android:layout_width="@dimen/_25sdp"
                    android:layout_height="@dimen/_25sdp"
                    android:layout_marginRight="@dimen/fivedp"
                    android:background="@drawable/ic_baseline_keyboard_arrow_up_24" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/expandable1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/fivedp"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    android:layout_marginVertical="@dimen/fivedp"
                    android:background="@drawable/bg_white"
                    android:orientation="vertical">

                    <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                        android:padding="@dimen/fivedp"
                        android:layout_margin="@dimen/fivedp"
                    android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/fivedp"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/textView59"
                                    android:layout_width="250dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:ellipsize="end"
                                    android:fontFamily="@font/euclid_circular_a_light"
                                    android:maxLines="1"
                                    android:textFontWeight="300"
                                    android:text="@string/latest_text"
                                    android:textColor="@color/textColor"
                                    android:textSize="@dimen/ninesp"
                                    android:textStyle="normal" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:gravity="right"
                                    android:layout_height="wrap_content">

                                    <ImageView
                                        android:layout_width="@dimen/dimen_12dp"
                                        android:layout_height="@dimen/dimen_12dp"
                                        android:tint="@color/font_icon_color"
                                        android:src="@drawable/ic_icon_metro_pin" />

                                </LinearLayout>
                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/firstLayoutNextPage"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="200dp"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:padding="2dp">


                                    <ImageView
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:src="@drawable/cx_icon_flag"
                                        android:text="TextView" />

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_gravity="center"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:layout_marginLeft="10dp"
                                            android:fontFamily="@font/euclid_circular_a_regular"
                                            android:gravity="center|left"
                                            android:text="IND"
                                            android:textColor="@color/black"
                                            android:textSize="@dimen/_14sp" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:layout_marginLeft="5dp"
                                            android:fontFamily="@font/euclid_circular_a_medium"
                                            android:gravity="center|left"
                                            android:text="55-3"
                                            android:textColor="@color/big_score_color"
                                            android:textSize="@dimen/dimen_20dp" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginLeft="3dp"
                                            android:fontFamily="@font/euclid_circular_a_light"
                                            android:gravity="center|left"
                                            android:text="14.4"
                                            android:textColor="@color/black"
                                            android:textSize="@dimen/elevensp" />

                                        <ImageView
                                            android:layout_width="@dimen/_12sdp"
                                            android:layout_gravity="center"
                                            android:rotation="-5"
                                            android:layout_marginBottom="2dp"
                                            android:layout_height="@dimen/_15sdp"
                                            android:layout_marginLeft="@dimen/_8sdp"
                                            android:src="@drawable/bat_icons"
                                            app:tint="#a76401" />

                                    </LinearLayout>

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:padding="2dp">


                                    <ImageView
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:src="@drawable/cx_icon_two"
                                        android:text="TextView" />

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_gravity="center"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:layout_marginLeft="10dp"
                                            android:fontFamily="@font/euclid_circular_a_regular"
                                            android:gravity="center|left"
                                            android:text="OMA"
                                            android:textColor="@color/black"
                                            android:textSize="@dimen/_14sp" />


                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="match_parent"
                                            android:layout_marginLeft="5dp"
                                            android:fontFamily="@font/euclid_circular_a_light"
                                            android:gravity="center|left"
                                            android:text="Yet to bat"
                                            android:textColor="@color/black"
                                            android:textSize="@dimen/ninesp" />

                                    </LinearLayout>

                                </LinearLayout>

                            </LinearLayout>


                            <View
                                android:id="@+id/view6"
                                android:layout_width="1dp"
                                android:layout_height="50dp"
                                android:layout_margin="5dp"
                                android:background="@drawable/bg_grey" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:orientation="vertical">


                                <TextView
                                    android:id="@+id/textView84"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/euclid_circular_a_light"
                                    android:gravity="center"
                                    android:text="Tomorrow"
                                    android:textColor="@color/textColor"
                                    android:textSize="@dimen/ninesp" />

                                <TextView
                                    android:id="@+id/textView85"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/poppins_medium"
                                    android:gravity="center"
                                    android:text="12:30PM"
                                    android:textColor="#C56749"
                                    android:textSize="@dimen/fifteen" />
                            </LinearLayout>

                        </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/tensp"
                            android:layout_weight="1"
                            android:fontFamily="@font/euclid_circular_a_regular"
                            android:text="Day 1:Session 1:"
                            android:textColor="@color/day_session"
                            android:textSize="@dimen/ninesp" />


                    </LinearLayout>

                    </LinearLayout>

